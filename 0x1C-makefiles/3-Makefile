# Implementing more rules and variables (cleaning).

CC = gcc
SRC = main.c school.c
OBJ = $(SRC:.c=.o)
NAME = school
RM = rm -f

all: m.h $(OBJ)
	$(CC) $(OBJ) -o $(NAME)

clean:
	$(RM) *~ school

oclean:
	$(RM) $(OBJ)

fclean:
	$(RM) *~ school
	$(RM) $(OBJ)

re:
	$(RM) $(OBJ)
	$(CC) $(OBJ) -o $(NAME)
